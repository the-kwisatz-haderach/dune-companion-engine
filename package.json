{
  "name": "dune",
  "version": "0.1.13",
  "main": "./dist/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib-esm/index.js",
  "files": [
    "dist",
    "lib",
    "lib-esm"
  ],
  "author": "Gustaf Lundstr√∂m",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build:lib:umd": "tsc",
    "build:lib:es": "tsc -m es6 --outDir lib-esm",
    "build:dist": "webpack",
    "build": "yarn run clean && yarn run build:lib:umd && yarn run build:lib:es && yarn run build:dist",
    "pack": "yarn build && yarn version --patch && yarn pack",
    "dev": "tsc --watch",
    "ts:defs": "tsc --declaration --outDir './build' --emitDeclarationOnly",
    "test": "jest --watchAll",
    "clean": "rm -rf lib lib-esm dist && rm *.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "prettier": "1.19.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^2.5.2"
  }
}
